        box-shadow: 0px 0px 10px #000000; -moz-box-shadow: 0px 0px 10px #000000; -webkit-box-shadow: 0px 0px 10px #000000;">
        <iframe id="Search_iView" name="Search_iView" height="240" src="/Resources/Framework/SearchViewCommon.html"
            frameborder="0" scrolling="no"></iframe>
    </div>
    <div id="Calendar_Layer" style="position: absolute; z-index: 30; display: none; width: 258px;
        height: 286px; margin-top: 5px;">
        <iframe id="frmCalendar" name="frmCalendar" src="/Resources/Framework/calendar.html"
            width="258px" height="286px" frameborder="0" scrolling="no"></iframe>
        <iframe id="frmCalendar_FT" name="frmCalendar_FT" src="/Resources/Framework/calendar_FT.html"
            style="width: 660px; height: 286px;" frameborder="0" scrolling="no"></iframe>
    </div>
    <div id="Image_Layer" style="display: none; z-index: 10;">
    </div>
    <div id="divOrgUser" style="display:none;">
        <div id="divOrgUser_close" onclick="grdOrgUser_Close()">×</div>
        <div id="divOrgUser_photo"></div>
            
        <ul style="float:left;">
