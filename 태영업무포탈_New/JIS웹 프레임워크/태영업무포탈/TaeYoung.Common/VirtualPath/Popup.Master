<body id="wrapper">
    <form id="form1" runat="server">
     <div style="display:none;">
        <input type="textbox" />
    </div>
    <script type="text/javascript" language="javascript" src="/Resources/Framework/JIS.Control.js"></script>
    <script type="text/javascript">
        var ControlLoad = [];
    </script>
    <!--서브시작-->
    <div id="sub_body">
        <!--상단시작-->
        <div id="header" style="display: none;">
            <!--탑소메뉴시작-->
            <div id="topsmenu_bx">
                <div id="user">
                    <strong>
                        <%= TaeYoung.Biz.Document.UserInfo.DeptName %>
                        <%= TaeYoung.Biz.Document.UserInfo.UserName%>님</strong>
                    <%= DateTime.Today.ToString("yyyy-MM-dd") %>
                    접속</div>
                <ul id="topsmenu">
                    <li id="help"><span title="HELP"><a href="javascript:OpenHelp();">?</a></span> </li>
                </ul>
            </div>
            <!--//탑소메뉴끝-->
        </div>
        <!--//상단끝-->
        <!--컨테이너시작-->
        <div id="container">
            <!--컨텐츠박스시작-->
            <div id="content_bx">
                <asp:ContentPlaceHolder ID="conBody" runat="server">
                </asp:ContentPlaceHolder>
            </div>
            <!--//컨텐츠박스끝-->
        </div>
        <!--//컨테이너끝-->
    </div>
