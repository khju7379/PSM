	    var IsPopup = true;
        var ProgramID = "<asp:LIteral ID="ltlProgramID" runat="server" />";
		var MenuID = "<asp:LIteral ID="ltlMenuID" runat="server" />";
        var EMPID = "<%= TaeYoung.Biz.Document.UserInfo.EmpID %>";
